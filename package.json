    "@types/y18n": "latest",
    "@vitejs/plugin-vue": "latest",
    "@vue/compiler-sfc": "latest",
    "acorn": "latest",
    "electron": "latest",
    "rollup": "latest",
    "socket.io": "latest",
    "stylus": "latest",
    "typescript": "latest",
    "vite": "latest",
